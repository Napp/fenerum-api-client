{
    "name": "napp/fenerum-api-client",
    "description": "API Client for Fenerum",
    "keywords": [
        "fenerum",
        "api",
        "webhooks"
    ],
    "homepage": "https://github.com/napp/fenerum-api-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Mads MÃ¸ller",
            "email": "mm@napp.dk",
            "homepage": "https://napp.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.4",
        "illuminate/events": "~5.8.0|^6.0",
        "illuminate/queue": "~5.8.0|^6.0",
        "illuminate/routing": "~5.8.0|^6.0",
        "illuminate/support": "~5.8.0|^6.0"
    },
    "require-dev": {
        "laravel/framework": "~5.8.0|^6.0",
        "nunomaduro/phpinsights": "^1.9",
        "phpunit/phpunit": "^8.2",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Fenerum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fenerum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
